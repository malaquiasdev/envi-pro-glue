{
  "Comment": "A Hello World example of the Amazon States Language using Pass states",
  "StartAt": "delete_database",
  "States": {
    "delete_database": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke",
      "Parameters": {
        "FunctionName": "arn:aws:lambda:us-east-1:252311082662:function:envi-pro-glue-dev-delete-vacant-table-step"
      },
      "Next": "create_new_database"
    },
    "create_new_database": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke",
      "Parameters": {
        "FunctionName": "arn:aws:lambda:us-east-1:252311082662:function:envi-pro-glue-dev-create-vacant-table-step"
      },
      "Next": "wait_five_seconds"
    },
    "wait_five_seconds": {
      "Type": "Wait",
      "Seconds": 5,
      "Next": "parallel_crawler_pciconcursos_pages"
    },
    "parallel_crawler_pciconcursos_pages": {
      "Type": "Parallel",
      "Branches": [
        {
          "StartAt": "geologia_page",
          "States": {
            "geologia_page": {
              "Type": "Task",
              "Resource": "arn:aws:states:::lambda:invoke",
              "Parameters": {
                "FunctionName": "arn:aws:lambda:us-east-1:252311082662:function:envi-pro-glue-dev-crawler-pciconcursos-vacant-page-step",
                "Payload": {
                  "category": "geologia"
                }
              },
              "End": true
            }
          }
        },
        {
          "StartAt": "geografia_page",
          "States": {
            "geografia_page": {
              "Type": "Task",
              "Resource": "arn:aws:states:::lambda:invoke",
              "Parameters": {
                "FunctionName": "arn:aws:lambda:us-east-1:252311082662:function:envi-pro-glue-dev-crawler-pciconcursos-vacant-page-step",
                "Payload": {
                  "category": "geografia"
                }
              },
              "End": true
            }
          }
        }
      ],
      "End": true
    }
  }
}
